{
    "name": "lovers-of-behat/table-extension",
    "description": "Behat extension to inspect HTML tables",
    "type": "behat-extension",
    "require": {
        "php": ">=7.1",
        "behat/behat": "^3.4",
        "symfony/dependency-injection": "^2.1 || ^3.0 || ^4.0 || ^5 || ^6" || "^7.1",
        "symfony/dom-crawler": "^2.1 || ^3.0 || ^4.0 || ^5 || ^6" || "^7.1",
        "symfony/event-dispatcher": "^2.1 || ^3.0 || ^4.0 || ^5 || ^6" || "^7.1",
        "friends-of-behat/mink-extension": "^2.7"
    },
    "license": ["MIT"],
    "authors": [
        {
            "name": "Pieter Frenssen",
            "email": "pieter@frenssen.be"
        }
    ],
    "autoload": {
        "psr-4": {
            "LoversOfBehat\\TableExtension\\": "src"
        }
    },
    "require-dev": {
        "behat/mink-goutte-driver": "^1.2 || ^2",
        "squizlabs/php_codesniffer": "^3.2",
        "symfony/css-selector": "^2.0|^3.0|^4.0"
    }
}
